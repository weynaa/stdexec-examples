{
	"version": 6,
	"configurePresets": [
		{
			"name": "x64-debug",
			"displayName": "debug",
			"generator": "Ninja",
			"binaryDir": "${sourceDir}/build",
			"installDir": "${sourceDir}/install",
			"cacheVariables": {
				"CMAKE_CXX_COMPILER": "cl.exe",
				"CMAKE_EXPORT_COMPILE_COMMANDS": true
			},
			"architecture": {
				"value": "x64",
				"strategy": "external"
			},
			"toolset": {
				"value": "host=x64",
				"strategy": "external"
			}
		}
	]
}